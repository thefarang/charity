{
  "name": "charity",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/standard \"src/**/*.js\"",
    "fix-style": "./node_modules/.bin/standard --fix \"src/**/*.js\"",
    "setup-app": "./node_modules/.bin/gulp",
    "build-assets": "npm run build-js && npm run build-css",
    "build-js": "./node_modules/.bin/gulp buildJS",
    "build-css": "./node_modules/.bin/gulp buildCSS",
    "debug": "./node_modules/.bin/nodemon ./src/bin/start.js",
    "start": "node ./src/bin/start.js"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.17.1",
    "bootstrap": "4.0.0",
    "bunyan": "1.8.12",
    "cookie-parser": "1.4.3",
    "config": "1.26.2",
    "ejs": "2.5.6",
    "elasticsearch": "14.1.0",
    "express": "4.15.2",
    "js-cookie": "2.2.0",
    "jquery": "3.3.1",
    "jsonwebtoken": "8.0.1",
    "mongoose": "4.11.12",
    "popper.js": "1.12.9",
    "serve-favicon": "2.4.2",
    "validate.js": "0.12.0"
  },
  "devDependencies": {
    "browserify": "15.2.0",
    "gulp": "3.9.1",
    "gulp-less": "3.4.0",
    "gulp-rename": "1.2.2",
    "nodemon": "1.12.1",
    "standard": "10.0.3",
    "vinyl-source-stream": "2.0.0"
  },
  "standard": {
    "ignore": [
      "src/public/javascripts/",
      "src/assets/javascripts/"
    ],
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it"
    ]
  }
}
