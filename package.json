{
  "name": "charity",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/standard \"src/**/*.js\"",
    "fix-style": "./node_modules/.bin/standard --fix \"src/**/*.js\"",
    "setup-app": "./node_modules/.bin/gulp",
    "build-js": "./node_modules/.bin/gulp buildJS",
    "build-css": "./node_modules/.bin/gulp buildCSS",
    "build-sitemap": "./node_modules/.bin/gulp buildSitemap",
    "build-robots": "./node_modules/.bin/gulp buildRobots",
    "test": "./node_modules/.bin/cucumber-js ./src/tests",
    "debug": "./node_modules/.bin/nodemon ./src/bin/start.js",
    "start": "node ./deploy/bin/start.js"
  },
  "dependencies": {
    "aws-sdk": "2.192.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.17.1",
    "bootstrap": "4.0.0",
    "bunyan": "1.8.12",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "config": "1.26.2",
    "ejs": "2.5.6",
    "elasticsearch": "14.1.0",
    "express": "4.15.2",
    "js-cookie": "2.2.0",
    "jquery": "3.3.1",
    "jsonwebtoken": "8.0.1",
    "lodash": "4.17.5",
    "mongoose": "4.11.12",
    "popper.js": "1.12.9",
    "serve-favicon": "2.4.2",
    "validate.js": "0.12.0"
  },
  "devDependencies": {
    "babel-loader": "7.1.2",
    "babel-core": "6.26.0",
    "babelify": "8.0.0",
    "babel-preset-es2015": "6.24.1",
    "browserify": "15.2.0",
    "chai": "4.1.2",
    "cucumber": "4.0.0",
    "gulp": "3.9.1",
    "gulp-clean-css": "3.9.2",
    "gulp-less": "3.4.0",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "nodemon": "1.12.1",
    "sitemap": "1.13.0",
    "standard": "10.0.3",
    "vinyl-source-stream": "2.0.0",
    "vinyl-buffer": "1.0.1",
    "watchify": "3.10.0"
  },
  "standard": {
    "ignore": [
      "src/public/javascripts/",
      "src/assets/javascripts/"
    ],
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it"
    ]
  }
}
