{
  "name": "charity",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/standard \"src/**/*.js\"",
    "fix-style": "./node_modules/.bin/standard --fix \"src/**/*.js\"",
    "init-users": "node ./src/scripts/init-users.js",
    "build-assets": "./node_modules/.bin/gulp",
    "debug": "./node_modules/.bin/nodemon ./src/bin/start.js",
    "start": "node ./src/bin/start.js"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.17.1",
    "bunyan": "1.8.12",
    "cookie-parser": "1.4.3",
    "config": "1.26.2",
    "ejs": "2.5.6",
    "express": "4.15.2",
    "express-sanitizer": "1.0.3",
    "jsonwebtoken": "8.0.1",
    "mongoose": "4.11.12",
    "serve-favicon": "2.4.2",
    "jquery": "3.3.1",
    "express-validator": "4.3.0"
  },
  "devDependencies": {
    "nodemon": "1.12.1",
    "standard": "10.0.3",
    "browserify": "15.2.0",
    "gulp": "3.9.1",
    "gulp-rename": "1.2.2",
    "vinyl-source-stream": "2.0.0"
  },
  "standard": {
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it"
    ],
    "ignore": [
      "./src/assets/javascripts/"
    ]
  }
}
